{
  "expo": {
    "name": "Evendi",
    "slug": "evendi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Evendi_app_icon.png",
    "scheme": "evendi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.norwedfilm.evendi"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0F1F3A",
        "foregroundImage": "./assets/images/Evendi_app_icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.norwedfilm.evendi"
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/Evendi_app_icon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Evendi_app_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0F1F3A",
          "dark": {
            "backgroundColor": "#0F1F3A"
          }
        }
      ],
      "expo-web-browser",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "reactCompiler": true
    }
  }
}
